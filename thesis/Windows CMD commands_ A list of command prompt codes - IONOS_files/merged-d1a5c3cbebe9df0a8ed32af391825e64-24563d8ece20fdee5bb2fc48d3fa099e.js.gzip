
$(function () {
    $(".search_results").slice(0, 10).show();
    $("#loadMore").on('click', function (e) {
        e.preventDefault();
        $(".search_results:hidden").slice(0, 10).slideDown();
        if ($(".search_results:hidden").length == 0) {
            $(".results_button").hide();
        }
    });
});

